<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the current release version of SequelPro.</string>
    <key>Identifier</key>
    <string>com.universalrobots.mac.munki.SequelPro</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>SequelPro</string>
        <key>MUNKI_REPO_SUBDIR</key>
        <string>apps/SequelPro</string>
        <key>APP_DESTINATION</key>
        <string>/Applications/Utilities</string>
        <key>pkginfo</key>
        <dict>
            <key>catalogs</key>
            <array>
                <string>testing</string>
            </array>
            <key>description</key>
            <string>Sequel Pro is a fast, easy-to-use Mac database management application for working with MySQL databases.</string>
            <key>display_name</key>
            <string>SequelPro</string>
            <key>name</key>
            <string>%NAME%</string>
            <key>unattended_install</key>
            <true/>
            <key>category</key>
            <string>Developer Tools</string>
            <key>developer</key>
            <string>CocoaMySQL</string>
            <key>minimum_os_version</key>
            <string>10.5</string>
        </dict>
    </dict>
    <key>MinimumVersion</key>
    <string>0.6.0</string>
    <key>ParentRecipe</key>
    <string>com.universalrobots.mac.download.SequelPro</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Arguments</key>
            <dict>
                <key>pkg_path</key>
                <string>%pathname%</string>
                <key>repo_subdirectory</key>
                <string>%MUNKI_REPO_SUBDIR%</string>
                <key>additional_makepkginfo_options</key>
                <array>
                    <string>--destinationpath</string>
                    <string>%APP_DESTINATION%</string>
                </array>
            </dict>
            <key>Processor</key>
            <string>MunkiImporter</string>
        </dict>
    </array>
</dict>
</plist>
